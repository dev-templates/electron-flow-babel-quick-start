  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/preset-flow": "^7.24.6",
    "babel-plugin-syntax-hermes-parser": "^0.21.1",
    "electron": "^30.0.9"
  },
  "pnpm": {
    "overrides": {
      "glob": "^10.0.0"
    }
  }
